{
  "name": "node-ts-starter",
  "version": "1.0.0",
  "main": "dist/api/app.bootstrap.js",
  "scripts": {
    "dev": "npm ts-node --respawn src/api/app.bootstrap.ts",
    "dev2": "ts-node-dev --respawn --transpile-only src/api/app.bootstrap.ts",
    "build": "tsc",
    "start": "node dist/api/app.bootstrap.js",
    "dev1": "ts-node -r tsconfig-paths/register src/api/app.bootstrap.ts",
    "start1": " npm run build && node -r tsconfig-paths/register dist/api/app.bootstrap.js",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write .",
    "test": "jest",
    "prisma:seed": "node prisma/seeds.js",
    "prisma:init": "npx prisma init",
    "prisma:migrate": "npx prisma migrate dev",
    "prisma:generate": "npx prisma generate",
    "prisma:studio": "npx prisma studio",
    "prisma:deploy": "npx prisma migrate deploy",
    "prisma:reset": "npx prisma migrate reset --force"
  },
  "_moduleAliases": {
    "@core": "dist/api/core",
    "@classes": "dist/api/core/types/classes",
